{
  "name": "react-pinch-and-zoom",
  "version": "1.0.1",
  "author": "Conrad Lo",
  "description": "A react container component with pinch-to-zoom gesture interaction.",
  "keywords": [
    "react",
    "reactjs",
    "pinch",
    "zoom",
    "pan"
  ],
  "files": [
    "lib/"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/conradlo/react-pinch-and-zoom"
  },
  "homepage": "https://github.com/conradlo/react-pinch-and-zoom#readme",
  "bugs": "https://github.com/conradlo/react-pinch-and-zoom/issues",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "test": "jest --watch",
    "dev": "concurrently \"npm run lib:watch\" \"npm start\"",
    "lib": "babel src/PinchToZoom -d lib --copy-files",
    "lib:watch": "babel src/PinchToZoom -w -d lib --copy-files",
    "build": "NODE_ENV=production npm run lib && webpack --mode production"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "morgan": "^1.9.0",
    "node-sass": "^4.10.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-hot-loader": "^4.1.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.1"
  },
  "peerDependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "dependencies": {
    "prop-types": "^15.6.1"
  }
}
